package view;
import controller.Metodos;
public class Principal {
	public static void main(){
		Metodos m = new Metodos();
		int[] vet1000 = new int[1000];
		for (int i = 0; i < vet1000.length; i++) {
            vet1000[i] = 0;
        }
		
		int[] vet10000 = new int[10000];
		for (int i = 0; i < vet10000.length; i++) {
            vet10000[i] = 0;
        }
		
		int[] vet100000 = new int[100000];
		for (int i = 0; i < vet100000.length; i++) {
            vet100000[i] = 0;
        }
	
		System.out.println("O tempo de espera para o vetor de 1000 foi de: "+m.vet1000(vet1000));
		System.out.println("O tempo de espera para o vetor de 10000 foi de: "+m.vet1000(vet10000));
		System.out.println("O tempo de espera para o vetor de 100000 foi de: "+m.vet1000(vet100000));
	}
}